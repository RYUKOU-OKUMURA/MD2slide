{
  "name": "@md2slide/frontend",
  "version": "0.1.0",
  "private": true,
  "description": "MD2slide Frontend - Next.js/React Web Editor",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
